<?xml version="1.0" encoding="utf-8" ?>
<!-- XSD manual extracted from package NLog.Schema: https://www.nuget.org/packages/NLog.Schema-->
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true"
      internalLogFile="logs/${date:format=yyyy-MM-dd}/${level}.${date:formate:HH}.log"
      internalLogLevel="Info" >

	<!-- the targets to write to -->
	<targets>
		<!-- write logs to file -->
		<target
			xsi:type="File"
			name="target1"
			fileName="logs/${date:format=yyyy-MM-dd}/${level}.${date:formate:HH}.log"
			layout="${date}|${level:uppercase=true}|${message}${exception:format=shorttype,message,stacktrace:maxInnerExceptionLevel=5:baseException=true:innerExceptionSeparator=&#13;&#10;------------INNER------------&#13;&#10;:innerFormat=type,message,data}|${logger}|${all-event-properties}"
		/>
		<target
			xsi:type="Console"
			name="target2"
			layout="${date}|${level:uppercase=true}|${message}${exception:format=shorttype,message,stacktrace:maxInnerExceptionLevel=5:baseException=true:innerExceptionSeparator=&#13;&#10;------------INNER------------&#13;&#10;:innerFormat=type,message,data}|${logger}|${all-event-properties}"
		/>
	</targets>

	<!-- rules to map from logger name to target -->
	<rules>
		<logger name="*" minlevel="Trace" writeTo="target1" />
		<logger name="*" minlevel="Trace" writeTo="target2" />
	</rules>
</nlog>